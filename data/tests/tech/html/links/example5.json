{
  "title": "HTML links example 5 - A link that wraps many elements",
  "description": "A link that wraps many elements",
  "html_file": "html/links.html",
  "assertions": [
    {
      "feature_id": "html/a(href)_element",
      "feature_assertion_id": "convey_role",
      "css_target": "#example-5 a[href]",
      "browserNotes": {
        "talkback": {
          "and_chr": "The entire content is announced as a link, and it is not possible to navigate content within the link. No semantics inside the link are conveyed."
        },
        "vo_macos": {
          "safari": "The entire contents of the link are announced when navigating to the link. Users can navigate into the link contain and navigate object by object."
        }
      }
    },
    {
      "feature_id": "html/a(href)_element",
      "feature_assertion_id": "convey_name",
      "css_target": "#example-5 a[href]",
      "browserNotes": {
        "talkback": {
          "and_chr": "The entire content is announced as a link, and it is not possible to navigate content within the link. No semantics inside the link are conveyed."
        },
        "vo_macos": {
          "safari": "The entire contents of the link are announced when navigating to the link. Users can navigate into the link contain and navigate object by object."
        }
      }
    },
    {
      "feature_id": "html/a(href)_element",
      "feature_assertion_id": "convey_boundaries",
      "css_target": "#example-5 a[href]",
      "browserNotes": {
        "jaws": {
          "chrome": "Announces the link role on every line, and does not announce something like \"entering link\" or \"leaving link\" when entering or leaving.",
          "ie": "Announces the link role on every line, and does not announce something like \"entering link\" or \"leaving link\" when entering or leaving.",
          "firefox": "Announces the link role on every line, and does not announce something like \"entering link\" or \"leaving link\" when entering or leaving."
        },
        "nvda": {
          "chrome": "Announces the link role on every line, and does not announce something like \"entering link\" or \"leaving link\" when entering or leaving.",
          "firefox": "Announces the link role on every line, and does not announce something like \"entering link\" or \"leaving link\" when entering or leaving."
        },
        "talkback": {
          "and_chr": "Boundaries implied by navigation. The entire content is announced as a link, and it is not possible to navigate content within the link. No semantics inside the link are conveyed."
        },
        "vo_macos": {
          "safari": "The entire contents of the link are announced when navigating to the link. Users can navigate into the link contain and navigate object by object."
        }
      }
    },
    {
      "feature_id": "html/a(href)_element",
      "feature_assertion_id": "provide_shortcuts",
      "css_target": "#example-5 a[href]",
      "browserNotes": {
        "talkback": {
          "and_chr": "The entire content is announced as a link, and it is not possible to navigate content within the link. No semantics inside the link are conveyed."
